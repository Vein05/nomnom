{
  "output": "./renamed",
  "case": "snake",
  "ai": {
    "provider": "deepseek",
    "model": "deepseek-v3",
    "api_key": "your api key",
    "ollama_model": "llama2",
    "max_tokens": 1000,
    "temperature": 0.7
  },
  "file_handling": {
    "include": ["pdf", "docx", "txt"],
    "exclude": ["mp4", "mp3"],
    "max_size": "100MB",
    "skip_errors": false,
    "keep_originals": false,
    "backup": false
  },
  "content_extraction": {
    "extract_text": true,
    "extract_metadata": true,
    "max_content_length": 5000,
    "skip_large_files": false,
    "read_context": true
  },
  "performance": {
    "workers": 4,
    "timeout": "30s",
    "retries": 3
  },
  "logging": {
    "log_level": "info",
    "log_file": "renamer.log",
    "no_color": false
  }
}
